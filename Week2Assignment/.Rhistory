pollutantmean <- function(directory, pollutant, id = 1:332) {
## 'directory' is a character vector of length 1 indicating
## the location of the CSV files
## 'pollutant' is a character vector of length 1 indicating
## the name of the pollutant for which we will calculate the
## mean; either "sulfate" or "nitrate".
## 'id' is an integer vector indicating the monitor ID numbers
## to be used
## Return the mean of the pollutant across all monitors list
## in the 'id' vector (ignoring NA values)
combidata <- data.frame()
for (i in id)
{
filename <-toString(directory)+"/"+
toString(formatC(i,width=3,format="d",flag="0"))
+".csv"
csvdata <- read.csv(filename)
merge(combidata, csvdata)
}
res <- mean(combidata$pollutant,na.rm = T)
return (res)
}
pollutantmean("specdata", "sulfate", 1:10)
pollutantmean <- function(directory, pollutant, id = 1:332) {
## 'directory' is a character vector of length 1 indicating
## the location of the CSV files
## 'pollutant' is a character vector of length 1 indicating
## the name of the pollutant for which we will calculate the
## mean; either "sulfate" or "nitrate".
## 'id' is an integer vector indicating the monitor ID numbers
## to be used
## Return the mean of the pollutant across all monitors list
## in the 'id' vector (ignoring NA values)
combidata <- data.frame()
for (i in id)
{
filename <- paste(directory,"/"
,formatC(i,width=3,format="d",flag="0")
,".csv"
,sep="")
csvdata <- read.csv(filename)
merge(combidata, csvdata)
}
res <- mean(combidata$pollutant,na.rm = T)
return (res)
}
